{
  "go.testFlags": ["-v"],
  "go.lintTool": "golangci-lint",
  "go.lintFlags": [
    "--fast"
  ],
  "go.testEnvFile": "${workspaceFolder}/dev.env",
  "cSpell.words": [
    "Arma",
    "BattlEye",
    "beparser",
    "beprinter",
    "bercon",
    "buffersize",
    "buildvcs",
    "cyclonedx",
    "dayz",
    "Debugf",
    "Debugln",
    "Errorf",
    "Errorln",
    "Fatalf",
    "Fatalln",
    "geoip",
    "GOARCH",
    "GOFLAGS",
    "gomod",
    "GOOS",
    "Infof",
    "Infoln",
    "ldflags",
    "loglevel",
    "logrus",
    "lzma",
    "mmdb",
    "oschwald",
    "rcon",
    "sirupsen",
    "Tracef",
    "Traceln",
    "trimpath",
    "urfave",
    "Warnf",
    "Warnln",
    "winres",
    "woozymasta"
  ],
  "cSpell.ignorePaths": [
    "go.mod", "go.sum", ".github/actions/**/*.yaml", ".github/workflows/**/*.yaml", ".vscode/*"
  ]
}
